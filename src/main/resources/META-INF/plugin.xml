<idea-plugin>
  <id>com.github.kunicmarko20.idea.helpable</id>
  <name>Helpable</name>
  <version>0.3.2</version>
  <vendor email="kunicmarko20@gmail.com" url="https://github.com/kunicmarko20">Marko Kunic</vendor>

  <idea-version since-build="192.0"/>

  <depends>com.jetbrains.php</depends>
  <depends>com.intellij.modules.platform</depends>

  <actions>
    <action id="com.github.kunicmarko20.idea.helpable.actions.generation.EqualsAction"
            class="com.github.kunicmarko20.idea.helpable.actions.generation.EqualsAction"
            text="Equals">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
    <action id="com.github.kunicmarko20.idea.helpable.actions.generation.ToAction"
            class="com.github.kunicmarko20.idea.helpable.actions.generation.ToAction"
            text="To*">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
    <action id="com.github.kunicmarko20.idea.helpable.actions.generation.WitherAction"
            class="com.github.kunicmarko20.idea.helpable.actions.generation.WitherAction"
            text="With*">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
    <action id="com.github.kunicmarko20.idea.helpable.actions.generation.FactoryMethodAction"
            class="com.github.kunicmarko20.idea.helpable.actions.generation.FactoryMethodAction"
            text="Factory Method">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
    <action id="com.github.kunicmarko20.idea.helpable.actions.generation.EnumAction"
            class="com.github.kunicmarko20.idea.helpable.actions.generation.EnumAction"
            text="Enum">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>

    <group id="JMSSerializerGroup" text="JMS Serializer" popup="true">
      <action id="com.github.kunicmarko20.idea.helpable.actions.generation.NewJmsConfigurationAction"
              class="com.github.kunicmarko20.idea.helpable.actions.generation.NewJmsConfigurationAction">
      </action>
      <action id="com.github.kunicmarko20.idea.helpable.actions.generation.NewJmsHandlerAction"
              class="com.github.kunicmarko20.idea.helpable.actions.generation.NewJmsHandlerAction">
      </action>
      <add-to-group group-id="NewGroup" anchor="last"/>
    </group>
  </actions>

</idea-plugin>
